{
  "name": "@tera-ui/use-real-shape",
  "version": "1.0.0",
  "description": "Hook that returns the real dimensions of an element",
  "keywords": [
    "use-real-shape"
  ],
  "author": "Tera Research Lab",
  "homepage": "https://tera-ui-storybook.vercel.app",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hieumau12/tera-ui.git",
    "directory": "packages/hooks/use-real-shape"
  },
  "bugs": {
    "url": "https://github.com/hieumau12/tera-ui/issues"
  },
  "scripts": {
