{
  "name": "@tera-ui/radio",
  "version": "1.2.1",
  "description": "Radios allow users to select a single option from a list of mutually exclusive options.",
  "keywords": [
    "radio"
  ],
  "author": "Tera Research Lab",
  "homepage": "https://tera-ui-storybook.vercel.app",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hieumau12/tera-ui.git",
    "directory": "packages/components/radio"
  },
  "bugs": {
    "url": "https://github.com/hieumau12/tera-ui/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "dev": "yarn build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "@tera-ui/theme": ">=1.0.0",
    "@tera-ui/system": ">=1.0.0"
  },
  "dependencies": {
    "@tera-ui/shared-utils": "workspace:*",
    "@tera-ui/react-utils": "workspace:*",
    "@tera-ui/use-aria-press": "workspace:*",
    "@react-aria/focus": "^3.14.3",
    "@react-aria/interactions": "^3.19.1",
    "@react-aria/radio": "^3.8.2",
    "@react-aria/utils": "^3.21.1",
    "@react-aria/visually-hidden": "^3.8.6",
    "@react-stately/radio": "^3.9.1",
    "@react-types/radio": "^3.5.2",
    "@react-types/shared": "^3.21.0"
  },
  "devDependencies": {
    "@tera-ui/theme": "workspace:*",
    "@tera-ui/system": "workspace:*",
    "@tera-ui/button": "workspace:*",
    "clean-package": "2.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "clean-package": "../../../clean-package.config.json"
}
